{
  "concurrency": 100,
  "recurse": true,
  "retry": true,
  "retryErrorsCount": 3,
  "retryErrorsJitter": 5,
  "skip": [
    ".*(\\.js|\\.css)$",
    "http://localhost",
    "https://github.com/libremfg/libremfg.github.io/edit/main/content/"
  ],
  "verbosity": "error",
  "format": "json",
  "statusCodes": {
    "403": "warn",
    "429": "warn",
    "5xx": "skip"
  }
}
